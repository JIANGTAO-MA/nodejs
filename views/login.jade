extends layout

block content
  h1= title
  p Welcome to #{title}
  div(id="login")
    el-form( :model = "loginForm"  label-width="100px" )
      el-form-item(label="账号" prop="username")
        el-input(v-model="loginForm.username" type="text" auto-complete="off")
      el-form-item(label="密码" prop="password")
        el-input(v-model="loginForm.password" type="password" auto-complete="off")
      el-form-item
        el-button( @click = 'submitForm("loginForm")' type = 'primary') 提交
        el-button( @click = 'resetForm("loginForm")' ) 重置